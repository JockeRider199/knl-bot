{
	"name": "knl-bot",
	"version": "0.1.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"husky:prepare": "husky install && husky add .husky/pre-commit \"pnpm lint-staged\"",
		"format": "prettier --write ./src/client/",
		"clean": "rm -rf dist",
		"bot:dev": "ts-node src/client/index.ts",
		"build": "tsc && cp -r prisma dist",
		"deploy": "docker buildx build --platform linux/arm64,linux/adm64 --push -f Dockerfile.Bot -t jockerider199/knl-bot:0.1.0 ."
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/ms": "^0.7.31",
		"@types/node": "^18.15.11",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"prisma": "^4.13.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@prisma/client": "^4.13.0",
		"discord.js": "^14.9.0",
		"dotenv": "^16.0.3",
		"ms": "^2.1.3",
		"poru": "^4.2.0"
	},
	"lint-staged": {
		"**/*.{ts,js,json}": [
			"prettier --write ./src/client/"
		],
		".prisma": "pnpm prisma format"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
